//isEven

 function isEven(num){
 	//return true if even
 	if(num % 2 === 0) {
 		return true;
 	} 
 	//return false otherwise
	else {
 		return false
 	}
 }

function isEven(num){
	return num % 2 === 0;
}

isEven(4); 
//console.log(isEven); 
isEven(21); 
//console.log(isEven); 
isEven(68);
//console.log(isEven); 
isEven(333);
//console.log(isEven); 